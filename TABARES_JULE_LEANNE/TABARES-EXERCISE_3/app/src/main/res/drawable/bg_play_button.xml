<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer glow -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#30FF1493" />
        </shape>
    </item>
    <!-- Inner solid -->
    <item
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#FF1493" />
        </shape>
    </item>
</layer-list>
